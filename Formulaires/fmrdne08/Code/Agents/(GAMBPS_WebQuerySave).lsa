			' Boucle sur les pays/bâtiments sélectionnés par le demandeur
			ForAll strTemp1 In pDocument.GAMBPS_Pays
				strKey = "AP-"+strTemp1
				
				' Document "Chef de Marché" (en théorie, un seul document de ce type existe)
				Set ValidMarcheDocument = ValidMarcheView.GetDocumentbykey(strKey, True)
				If ValidMarcheDocument Is Nothing Then Error lsERR_NOTES_DOC_NOTINVIEW, "Impossible d'ouvrir le document de paramétrage ""Chef de Marche"" !"
				
				' Correspondant Chef de marché
				Set TempItem = ValidMarcheDocument.GetFirstItem("GAMBPS_ValidMarche_Name")
				If TempItem Is Nothing Then Error lsERR_NOTES_ITEM_MISSING, "Impossible de trouver le champ de paramétrage ""GAMBPS_ValidMarche_Name"" !"
				
				' Lecteurs: ajout des valideurs
				Call iLect.AppendToTextList(TempItem.Values)
				pDocument.f_LecteursPotentiels = Evaluate(|@Sort(@Unique(@Trim(f_LecteursPotentiels)))|, pDocument)
				
				' Envoi du mail (BAL nominative ou fonctionnelle)
				If Trim(UCase(ValidMarcheDocument.GAMBPS_SendMail(0))) = "BALNOM" Then
					Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, ValidMarcheDocument.GAMBPS_ValidMarche_Name, Null, "ADD4")
				ElseIf Trim(UCase(ValidMarcheDocument.GAMBPS_SendMail(0))) = "BALFON" Then
					Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, ValidMarcheDocument.GAMBPS_ValidMarche_Balfon(0), Null, "ADD4")
				End If	
			End ForAll
		End If
		
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	
		' E5: Valideur autorisé =&gt; PDST
		If (CurrentStep &gt;= 5) Then
			
			' Ouverture de la vue contenant le document "chef produit DPMX"
			Set ValidDPMXView = ParamDB.GetView("v_GAMBPS_ValidDPMX")
			If ValidDPMXView Is Nothing Then Error lsERR_NOTES_VIEWOPEN_FAILED, "Impossible d'ouvrir la vue ""v_GAMBPS_ValidDPMX"" sur la base de paramétrage !"
			
			' Document "Chef produit DPMX" (en théorie, un seul document de ce type existe)
			Set ValidDPMXDocument = ValidDPMXView.GetDocumentbykey(pDocument.GAMBPS_Vehicule(0), True)
			If ValidDPMXDocument Is Nothing Then Error lsERR_NOTES_DOC_NOTINVIEW, "Impossible d'ouvrir le document de paramétrage ""Chef produit DPMX"" !"
			
			' Correspondant Chef produit DPMX
			Set TempItem = ValidDPMXDocument.GetFirstItem("GAMBPS_ValidDPMX_Name")
			If TempItem Is Nothing Then Error lsERR_NOTES_ITEM_MISSING, "Impossible de trouver le champ de paramétrage ""GAMBPS_ValidDPMX_Name"" !"
			
			' Lecteurs: ajout des valideurs
			Call iLect.AppendToTextList(TempItem.Values)	
			pDocument.f_LecteursPotentiels = Evaluate(|@Sort(@Unique(@Trim(f_LecteursPotentiels)))|, pDocument)
			
			' Envoi du mail (BAL nominative ou fonctionnelle)
			If Trim(UCase(ValidDPMXDocument.GAMBPS_SendMail(0))) = "BALNOM" Then
				Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, ValidDPMXDocument.GAMBPS_ValidDPMX_Name, Null, "ADD4")
			ElseIf Trim(UCase(ValidDPMXDocument.GAMBPS_SendMail(0))) = "BALFON" Then
				Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, ValidDPMXDocument.GAMBPS_ValidDPMX_Balfon(0), Null, "ADD4")
			End If		
		End If
	
	' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
	ElseIf UCase(TypeMarque$) = "AC" Then
	
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
		' E4: Valideur autorisé =&gt; Chef de Marché
		If (CurrentStep &gt;= 4) Then
			
			'Ouverture de la vue contenant le document "Chef de Gamme"
			Set ValidGammeView = ParamDB.GetView("v_GAMBPS_ValidGamme")
			If ValidGammeView Is Nothing Then Error lsERR_NOTES_VIEWOPEN_FAILED, "Impossible d'ouvrir la vue ""v_GAMBPS_ValidGamme"" sur la base de paramétrage !"

			'Document "Chef de Gamme" (en théorie, un seul document de ce type existe)
			Set ValidGammeDocument = ValidGammeView.GetDocumentbykey(pDocument.GAMBPS_Vehicule(0), True)
			If ValidGammeDocument Is Nothing Then Error lsERR_NOTES_DOC_NOTINVIEW, "Impossible d'ouvrir le document de paramétrage ""Chef de Gamme"" !"

			'Correspondant Chef de Gamme
			Set TempItem = ValidGammeDocument.GetFirstItem("GAMBPS_ValidGamme_Name")
			If TempItem Is Nothing Then Error lsERR_NOTES_ITEM_MISSING,_
			"Impossible de trouver le champ de paramétrage ""GAMBPS_ValidGamme_Name"" !"
			
			' Lecteurs: ajout des valideurs
			Call iLect.AppendToTextList(TempItem.Values)	
			pDocument.f_LecteursPotentiels = Evaluate(|@Sort(@Unique(@Trim(f_LecteursPotentiels)))|, pDocument)
			
			' Envoi du mail (BAL nominative ou fonctionnelle)
			If Trim(UCase(ValidGammeDocument.GAMBPS_SendMail(0))) = "BALNOM" Then
				Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, ValidGammeDocument.GAMBPS_ValidGamme_Name, Null, "ADD4")
			ElseIf Trim(UCase(ValidGammeDocument.GAMBPS_SendMail(0))) = "BALFON" Then
				Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, ValidGammeDocument.GAMBPS_ValidGamme_Balfon(0), Null, "ADD4")
			End If
		End If
		
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	
		' E5: Valideur autorisé =&gt; PDST
		If (CurrentStep &gt;= 5) Then
			
			' Ouverture de la vue contenant le document "Chef de Marche"
			Set ValidMarcheView = ParamDB.GetView("v_GAMBPS_ValidMarche_Lookup_ByKey")
			If ValidMarcheView Is Nothing Then Error lsERR_NOTES_VIEWOPEN_FAILED, "Impossible d'ouvrir la vue ""v_GAMBPS_ValidMarche_Lookup_ByKey"" sur la base de paramétrage !"
			
			' Boucle sur les pays/bâtiments sélectionnés par le demandeur
			ForAll strTemp1 In pDocument.GAMBPS_Pays
				strKey = "AC-"+strTemp1
				
				' Document "Chef de Marché" (en théorie, un seul document de ce type existe)
				Set ValidMarcheDocument = ValidMarcheView.GetDocumentbykey(strKey, True)
				If ValidMarcheDocument Is Nothing Then Error lsERR_NOTES_DOC_NOTINVIEW, "Impossible d'ouvrir le document de paramétrage ""Chef de Marche"" !"
				
				' Correspondant Chef de marché
				Set TempItem = ValidMarcheDocument.GetFirstItem("GAMBPS_ValidMarche_Name")
				If TempItem Is Nothing Then Error lsERR_NOTES_ITEM_MISSING, "Impossible de trouver le champ de paramétrage ""GAMBPS_ValidMarche_Name"" !"
				
				' Lecteurs: ajout des valideurs
				Call iLect.AppendToTextList(TempItem.Values)	
				pDocument.f_LecteursPotentiels = Evaluate(|@Sort(@Unique(@Trim(f_LecteursPotentiels)))|, pDocument)
				
				' Envoi du mail (BAL nominative ou fonctionnelle)
				If Trim(UCase(ValidMarcheDocument.GAMBPS_SendMail(0))) = "BALNOM" Then
					Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, ValidMarcheDocument.GAMBPS_ValidMarche_Name, Null, "ADD4")
				ElseIf Trim(UCase(ValidMarcheDocument.GAMBPS_SendMail(0))) = "BALFON" Then
					Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, ValidMarcheDocument.GAMBPS_ValidMarche_Balfon(0), Null, "ADD4")
				End If
			End ForAll
		End If
	End If
	
	'- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	
	'E6: Valideur autorisé =&gt; Service BPS
	If (CurrentStep &gt;= 6) Then
		

		' Ouverture de la vue contenant le document "PDST"
		Set ValidPDSTView = ParamDB.GetView("v_GAMBPS_ValidPDST")
		If ValidPDSTView Is Nothing Then Error lsERR_NOTES_VIEWOPEN_FAILED, "Impossible d'ouvrir la vue ""v_GAMBPS_ValidPDST"" sur la base de paramétrage !"
		
		' Document "PDST" (en théorie, un seul document de ce type existe)
		Set ValidPDSTDocument = ValidPDSTView.GetDocumentbykey(pDocument.GAMBPS_Vehicule(0), True)
		If ValidPDSTDocument Is Nothing Then Error lsERR_NOTES_DOC_NOTINVIEW, "Impossible d'ouvrir le document de paramétrage ""PDST"" !"
		
		' Correspondant PDST
		Set TempItem = ValidPDSTDocument.GetFirstItem("GAMBPS_ValidPDST_Name")
		If TempItem Is Nothing Then Error lsERR_NOTES_ITEM_MISSING, "Impossible de trouver le champ de paramétrage ""GAMBPS_ValidPDST_Name"" !"
		
		' Lecteurs: ajout des valideurs
		Call iLect.AppendToTextList(TempItem.Values)	
		pDocument.f_LecteursPotentiels = Evaluate(|@Sort(@Unique(@Trim(f_LecteursPotentiels)))|, pDocument)
		
		' Envoi du mail (BAL nominative ou fonctionnelle)
		If Trim(Ucase(ValidPDSTDocument.GAMBPS_SendMail(0))) = "BALNOM" Then
			Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, ValidPDSTDocument.GAMBPS_ValidPDST_Name, Null, "ADD4")
		Elseif Trim(Ucase(ValidPDSTDocument.GAMBPS_SendMail(0))) = "BALFON" Then
			Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, ValidPDSTDocument.GAMBPS_ValidPDST_Balfon(0), Null, "ADD4")
		End If
		
	End If
	
	'Valideur autorisé
	pDocument.f_AuteursPotentiels = "[AdminForms]"
	
	'Changement d'état, état 98
	Call ChangeState(pDocument, pFormParamsDocument, 98, "Terminal")
	pDocument.f_EtatTerminal = "O"
	
	'Dispaching des acteurs (groupes ou individus)
	pDocument.f_ActeursIndividu = ""
	pDocument.f_ActeursGroupe = ""
	pDocument.ActeursMembresGroupe = ""
	
	'C'est fini, on sort
	Exit Sub
	
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	' 															GESTION D'ERREUR
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
ErrorHandler:
	
	' On propage les erreurs si neccessaire
	If (Err = ERROR_PROPAGATE%) Then
		Error ERROR_PROPAGATE%, Error$
		
	Else
		Dim LogEntry As New LogEntry
		LogEntry.LogType		= LOGLEVEL_EMERG%
		LogEntry.SubName		= "GAMBPS_DenyDocument"
		LogEntry.Erl 			= Erl
		LogEntry.Err			= Err
		LogEntry.Msg			= Error$
		
		' On fait remonter l'erreur
		Error ERROR_PROPAGATE%, LogEntry.ToString
		
	End If
	
	Exit Sub
	
End Sub

