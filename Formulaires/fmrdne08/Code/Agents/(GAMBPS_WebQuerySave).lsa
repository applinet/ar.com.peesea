	TypeMarque$ = pDocument.GAMBPS_Marque(0)
		
	' Marque: AP=Peugeot / AC=Citroen
	If UCase(TypeMarque$) = "AP" Then
		
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	
