	On Error Goto ErrorHandler
	
	
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	' 																INITIALISATION
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	
	' Variables locales
	Dim CurrentStep As Integer
	Dim TempItem As NotesItem, iLect As NotesItem
	Dim TypeMarque As String
	Dim strKey As String
	
	'Attribuée: (FMR-602) GAMBPS : Validation d'étape par "import" 07/10/2010 by Juan Bautista Cabral.
	Dim vVehicules As NotesView
	Dim dVehicule As NotesDocument
	Dim UserRejectFile As NotesName
	
	' Base de paramétrage
	Dim ParamDB As New NotesDatabase(pProfileDocument.f_Serveur_Parametre(0), pProfileDocument.f_Path_Parametre(0))
	If Not ParamDB.IsOpen Then Error lsERR_NOTES_DBOPEN_FAILED, "Impossible d'ouvrir la base """ + pProfileDocument.f_Path_Parametre(0) + """ sur le serveur """ + pProfileDocument.f_Serveur_Parametre(0) + """"
	
	
	' Etat de la demande	
	CurrentStep = Cint(pDocument.f_Etat(0))
	
	' Vues de documents de paramétrage
	Dim ValidMarcheView As NotesView, ValidDPMXView As NotesView, ValidPDSTView As NotesView, ValidGammeView As NotesView
	
	' Documents de paramétrage
	Dim ValidMarcheDocument As NotesDocument, ValidDPMXDocument As NotesDocument, ValidPDSTDocument As NotesDocument, ValidGammeDocument As NotesDocument
	
	' Construction du demandeur	
	Dim Demandeur As NotesName
	Set Demandeur = New NotesName(pDocument.f_Demandeur(0))
	
	' Construction du Autre Personne habilitée
	Dim AutrePersonneHab As NotesName
	
