				Call pDocument.ReplaceItemValue("GAMBPS_reg_"+Cstr(index),"")	
			Else
				Call pDocument.ReplaceItemValue("GAMBPS_reg_"+Cstr(index),aRegTemp(index))
			End If
'Equipements en plus 	
			If index &gt; k2 Then
				Call pDocument.ReplaceItemValue("GAMBPS_equiplus_"+Cstr(index),"")	
			Else
				Call pDocument.ReplaceItemValue("GAMBPS_equiplus_"+Cstr(index),aPlusTemp(index))
			End If
'Equipements en moins 	
			If index &gt; k3 Then
				Call pDocument.ReplaceItemValue("GAMBPS_equimoins_"+Cstr(index),"")	
			Else
				Call pDocument.ReplaceItemValue("GAMBPS_equimoins_"+Cstr(index),aMoinsTemp(index))
			End If
		Next
		
		' Réengagement (au sens classique Formulaires)
		'If pDocument.A_Complementer(0) &lt;&gt; "O" Then
		'	On Error lsERR_NOTES_BAD_UNID Resume Next
		'	If(pDocument.ParentDocUNID(0) &lt;&gt;"") Then
		'		Set ParentDoc = pDB.GetDocumentByUNID(pDocument.ParentDocUNID(0))
		'		If Not ParentDoc Is Nothing Then
					'Call ParentDoc.Remove(True)
		'		End If
		'	End If		
		'End If
		
	End If
	
	Exit Sub
	
	
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
