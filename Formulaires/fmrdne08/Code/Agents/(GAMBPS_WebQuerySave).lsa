'++LotusScript Development Environment:2:2:GAMBPS_ComplementerDocument:1:8
Sub GAMBPS_ComplementerDocument(pDB As NotesDatabase, pFormParamsDocument As NotesDocument, pProfileDocument As NotesDocument, pDocument As NotesDocument, pUserName As NotesName)
	' Gestion d'erreurs	
	On Error Goto ErrorHandler
	
	Dim CurrentStep As Integer
	' Construction du demandeur	
	Dim Demandeur As NotesName
	Set Demandeur = New NotesName(pDocument.f_Demandeur(0))
	
	' Autre Personne habilitée
	Dim AutrePersonneHab As NotesName
	Dim iAutre As NotesItem
	
	' Etat de la demande	
	CurrentStep = Cint(pDocument.f_Etat(0))
	
	If pDocument.f_TypeEtat(0) = "Intermediaire" Then
		' Historique
		Call AddHistLine(pDocument, "l_GAMBPS_Histo_E96", pUserName.Abbreviated)
		
		' Correspondant Demandeur
		Call pDocument.ReplaceItemValue("f_AuteursPotentiels", Demandeur.Canonical)
		
		' Correspondant Autre Personne habilitée		
		If (pDocument.GAMBPS_Autre_per_hab(0) &lt;&gt; "" ) Then
			Set AutrePersonneHab = New NotesName(pDocument.GAMBPS_Autre_per_hab(0))
			Set iAutre = pDocument.GetFirstItem("f_AuteursPotentiels")
			Call iAutre.AppendToTextList(AutrePersonneHab.Canonical)
			
			' Envoi du mail (Autre Personne habilitée)
			Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, AutrePersonneHab.Canonical, Null, "ADD3")
		End If
				
		' Changement d'état, etat 96
		Call ChangeState(pDocument, pFormParamsDocument, 96, "Intermediaire")
		' Dispaching des acteurs (groupes ou individus)
		Call DispatchActors(pDocument)
		
		pDocument.A_Complementer = "O"
		Call pDocument.ReplaceItemValue("Forms_E" + Cstr(CurrentStep) + "_Avis", "")
		
		' Envoi du mail (Demandeur)
		Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, Demandeur.Canonical, Null, "ADD3")	
		Call pDocument.ReplaceItemValue("Forms_LastValidSendTo", pDocument.f_Demandeur(0))
	End If
	
	Exit Sub
	
	
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	' 															GESTION D'ERREUR
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
ErrorHandler:
	
	' On propage les erreurs si neccessaire
	If (Err = ERROR_PROPAGATE%) Then
		Error ERROR_PROPAGATE%, Error$
		
	Else
		Dim LogEntry As New LogEntry
		LogEntry.LogType		= LOGLEVEL_EMERG%
		LogEntry.SubName		= "GAMBPS_StepUpDocument"
		LogEntry.Erl 			= Erl
		LogEntry.Err			= Err
		LogEntry.Msg			= Error$
		
		' On fait remonter l'erreur
		Error ERROR_PROPAGATE%, LogEntry.ToString
		
	End If
	
	Exit Sub
	
End Sub

'++LotusScript Development Environment:2:1:CalculeNumeroOrdre:1:8
Function CalculeNumeroOrdre(nOrdre As Long) As String
	' Gestion d'erreurs
	On Error Goto ErrorHandler
	
	If (nOrdre&lt;10)Then
		CalculeNumeroOrdre = "000000" + Cstr(nOrdre)			
	Elseif (nOrdre&lt;100)Then		
		CalculeNumeroOrdre = "00000" + Cstr(nOrdre)	
	Elseif (nOrdre&lt;1000)Then		
		CalculeNumeroOrdre = "0000" + Cstr(nOrdre)	
	Elseif (nOrdre&lt;10000)Then		
		CalculeNumeroOrdre = "000" + Cstr(nOrdre)	
	Elseif (nOrdre&lt;100000)Then		
		CalculeNumeroOrdre = "00" + Cstr(nOrdre)	
	Elseif (nOrdre&lt;1000000)Then		
		CalculeNumeroOrdre = "0" + Cstr(nOrdre)	
	Elseif (nOrdre&lt;10000000)Then		
		CalculeNumeroOrdre = Cstr(nOrdre)			
	Else
		CalculeNumeroOrdre = ""
	End If
	
	Exit Function
	
	
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
		' 											Gestion d'erreurs
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
ErrorHandler:
	
	
	' On propage les erreurs si neccessaire
	If (Err = ERROR_PROPAGATE%) Then
		Error ERROR_PROPAGATE%, Error$
		
	Else
		Dim LogEntry As New LogEntry
		LogEntry.LogType			= LOGLEVEL_EMERG%
		LogEntry.SubName		= "CalculeNumeroOrdre"
		LogEntry.Erl 			= Erl
		LogEntry.Err			= Err
		LogEntry.Msg			= Error$
		
		' On fait remonter l'erreur
		Error ERROR_PROPAGATE%, LogEntry.ToString
		
	End If
	Exit Function
	
	
End Function

'++LotusScript Development Environment:2:2:InitializeImport:1:8
Sub InitializeImport( Database As notesDatabase, ProfileDocument As NotesDocument, ContextDocument As notesdocument, Process As Process)
%REM		Created by N. Laurenzano - 16.09.2010 - FMR-602
Gets the imported CSV file and loops through all its lines.
For each valid line calls the InitializeDocument sub, to make the proper changes to the workflow.
	- A valid line has a value in column 66 (BN) that is either "SUCCÈS" or "ECHEC".
	- A second validation is that the Reference number in colunm 5 (E) must correspond to a document of type GAMBPS with status equal to 6.
%END REM
	
	' Gestion d'erreurs	
	On Error Goto ErrorHandler
	
	
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	' 										INITIALISATION
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	
	' Vue des documents à importer
	Dim view As NotesView
	Set view = Database.GetView("v_GAMBPS_Lookup_ByNumber")
	If view Is Nothing Then Error lsERR_NOTES_VIEWOPEN_FAILED, "Impossible d'ouvrir la vue ""v_GAMBPS_Lookup_ByNumber"""
	
	Dim docParamProfileDocument As NotesDocument
	Dim TempDocument As NotesDocument
	Dim TempItem As NotesItem
	
	Dim AttachmentNames As Variant
	Dim AttachmentItem As NotesEmbeddedObject
	
	Dim FileName As String
	Dim FileNum As Integer
	Dim LineStr As String
	Dim TempVar As Variant
	Dim TempName As NotesName
	Dim i As Integer
	Dim strReference, strAction, strComment As String
	
	
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	' 										TRAITEMENT
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	
	
	' On récupère la liste des pièces jointes
	AttachmentNames = Evaluate(|@AttachmentNames|, ContextDocument)
	
	' On boucle sur les pièces jointes
	Forall AttachmentName In AttachmentNames
		
		' On récupère la pièce jointe
		Set AttachmentItem = ContextDocument.GetAttachment(Cstr(AttachmentName))
		If Not AttachmentItem Is Nothing Then
			
			' On extrait la pièce jointe dans le dossier temporaire
			
			' Base de paramétrage
			Dim ParamDB As New NotesDatabase(ProfileDocument.f_Serveur_Parametre(0), ProfileDocument.f_Path_Parametre(0))
			If Not ParamDB.IsOpen Then Error lsERR_NOTES_DBOPEN_FAILED, "Impossible d'ouvrir la base """ + ProfileDocument.f_Path_Parametre(0) + """ sur le serveur """ + ProfileDocument.f_Serveur_Parametre(0) + """ !"
			
			Set docParamProfileDocument = ParamDB.GetProfileDocument("m_ProfileParam")
			If docParamProfileDocument Is Nothing Then Error lsERR_NOTES_NOSUCH_PROFILE, "Impossible d'ouvrir le document de profil de la base """ + ProfileDocument.f_Path_Parametre(0) + """ sur le serveur """ + ProfileDocument.f_Serveur_Parametre(0) + """ !"
			
			
'			FileName = "/usersdev/fmr00/agent" + "/" + AttachmentItem.Source
			FileName = docParamProfileDocument.Forms_TempDir(0) + "/" + AttachmentItem.Source
			Call AttachmentItem.ExtractFile(FileName)
			
			' Ouverture du fichier
			FileNum = Freefile
			Open FileName For Input As FileNum
			
			' Si le fichier contient une ligne d'entête, on la lit (juste pour déplacer le pointeur de fichier à la ligne suivante)
	'		If Trim(Ucase(ContextDocument.UIImportDialog_Header(0))) = "Y" Then
			' This type of form always has a header in the .csv file
			Line Input #FileNum%, LineStr
	'		End If
			
			Call view.Refresh
			view.AutoUpdate = False
			
			' Lecture du fichier ligne par ligne
			Do While Not Eof(FileNum%)
				' Récupération d'une ligne
				Line Input #FileNum%, LineStr
				
				' On découpe la ligne
				TempVar = Split(LineStr, ";")
				
				' On doit avoir 66 éléments
				If (Ubound(TempVar) &gt;= 65) Then
					strReference = Ucase(Trim(TempVar(4)))
					' If there is no reference number, the line is not processed
					If strReference &lt;&gt; "" Then
						' Si besoin, on vérifie que le code n'existe pas pour ne pas l'écraser
						Set TempDocument = view.GetDocumentByKey(strReference, True)	' Column 5 = Reference
						
						' If the document is not found, the line is not processed
						If Not TempDocument Is Nothing Then
							'If the document status is not "Waiting for Mise à jour BPS verification" and the form is not of GAMBPS type, the line is not processed
							If (TempDocument.GetItemValue("f_Etat")(0) = 6 And TempDocument.GetItemValue("f_TypeForm")(0) = "GAMBPS") Then
								strAction = Ucase(Trim(TempVar(65)))
							
								If (strAction &lt;&gt; "") And (strAction = "SUCCÈS" Or strAction = "ECHEC") Then
									TempDocument.Forms_E6_DateApprob = Now
							
									' Checks the column with the comments
									If (Ubound(TempVar) &gt;= 66) Then
										strComment = TempVar(66)
									Else
										strComment = ""
									End If
									
									If strAction = "SUCCÈS" Then
										TempDocument.f_ActionAFaire = "soumettre"
										TempDocument.Forms_E6_Avis = "1"
										TempDocument.Forms_E6_Comments = strComment
									Elseif strAction = "ECHEC" Then
										TempDocument.f_ActionAFaire = "refuser"
										TempDocument.Forms_E6_Avis = "0"
										
										If Trim(strComment) = "" Then
											TempDocument.Forms_E6_Comments = "LA DEMANDE N'A PAS PU ETRE TRAITEE EN BPS"
										Else
											TempDocument.Forms_E6_Comments = strComment
										End If
									End If
									
									Call InitializeDocument( Database, ProfileDocument, TempDocument, ContextDocument, Process )
								End If
								
							End If		' (TempDocument.GetItemValue("f_Etat")(0) = 6 And TempDocument.GetItemValue("f_TypeForm")(0) = "GAMBPS")
						End If	' Not TempDocument Is Nothing
					End If		' strReference &lt;&gt; ""
				End If			' (Ubound(TempVar) &gt;= 65)
				
			Loop
			
			' Fermeture et suppression du fichier
			Close FileNum
			Kill FileName
			
		End If
		
	End Forall	
	
	
	' Redirection
	Print "&lt;script&gt;"
	Print "var url = window.location.href;"
	Print "window.location.href = url.substring(0,(url.lastIndexOf('.nsf')+5)) + 'v_GAMBPS_Suivre?OpenView'"
	Print "&lt;/script&gt;"
	
	
	' C'est fini, on sort
	Exit Sub
	
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	' 										GESTION D'ERREURS
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	
ErrorHandler:
	
	Dim LogEntry As New LogEntry
	
	' Erreur propagée
	If (Err = ERROR_PROPAGATE%) Then
		
		' On charge les valeurs depuis le message d'erreur
		LogEntry.FromString(Error$)
		
	Else
		
		' On log l'erreur
		LogEntry.LogType			= LOGLEVEL_EMERG
		LogEntry.SubName		= "InitializeImport"
		LogEntry.Erl 			= Erl
		LogEntry.Err			= Err
		LogEntry.Msg			= Error$
		
	End If
	
	' Fermeture et suppression du fichier
	Close FileNum
	Kill FileName
	
	LogEntry.Component = "GAMBPS"
	Call LogEntry.Save(ProfileDocument.f_Serveur_Log(0), ProfileDocument.f_Path_Log(0))
	
	Print "Une ou plusieurs erreurs critiques se sont produites !&lt;br&gt;Les erreurs ont été loguées.";
	
	Exit Sub
	
End Sub

'++LotusScript Development Environment:2:2:InitializeDocument:1:8
Sub InitializeDocument( CurrentDB As notesDatabase, ProfileDocument As NotesDocument, CurrentDoc As NotesDocument, ContextDoc As NotesDocument, Process As Process)
%REM		Created by N. Laurenzano - 16.09.2010 - FMR-602
Manages the workflow changes for a single document.
This code was originally in the Initialize sub, but was moved here so the agent could be
used for manually submitting a single document and for making status changes based on an imported CSV file.
%END REM
	
	Dim CurrentUser As NotesName
	Dim CurrentLng As String
	
	' Gestion d'erreurs
	On Error Goto ErrorHandler
	
	Set CurrentUser = New NotesName(ContextDoc.Remote_User(0))
	
	
	CurrentLng = CurrentDoc.f_lng(0)
	
	' Processus de traitement
	Process.Status = PROCESSSTATUS_SUCCESS%
	
	' Vue des documents de paramétrage
	Dim ParamsView As NotesView
	Set ParamsView = CurrentDB.GetView("v_FormsParams")
	If ParamsView Is Nothing Then Error lsERR_NOTES_VIEWOPEN_FAILED, "Impossible d'ouvrir la vue ""v_FormsParams"" !"
	
	' Document de paramétrage pour le type du formulaire
	Dim ParamsDocument As NotesDocument
	Set ParamsDocument = ParamsView.GetDocumentbyKey(Trim(Ucase(CurrentDoc.f_TypeForm(0))))
	If ParamsDocument Is Nothing Then Error lsERR_NOTES_DOC_NOTINVIEW, "Impossible d'ouvrir le document de paramétrage du formulaire """ + Trim(Ucase(CurrentDoc.f_TypeForm(0))) + """ !"
	
	
	Dim TempItem As NotesItem
	
	
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	' 																TRAITEMENT
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *	
	
	' Soumettre/Confirmer
	If Trim(Lcase(CurrentDoc.f_ActionAFaire(0))) = "soumettre" Then
		Process.Action = PROCESSACTION_SUBMIT%
		Call GAMBPS_StepUpDocument(CurrentDB, ParamsDocument, ProfileDocument, CurrentDoc, CurrentUser)
		
	' Refuser
	Elseif Trim(Lcase(CurrentDoc.f_ActionAFaire(0))) = "refuser" Then
		Process.Action = PROCESSACTION_DENY%
		Call GAMBPS_DenyDocument(CurrentDB, ParamsDocument, ProfileDocument, CurrentDoc, CurrentUser)
		
		
	' Annuler
	Elseif Trim(Lcase(CurrentDoc.f_ActionAFaire(0))) = "annuler" Then
		Process.Action = PROCESSACTION_CANCEL%
		Call GAMBPS_CancelDocument(CurrentDB, ParamsDocument, ProfileDocument, CurrentDoc, CurrentUser)		
		
		
	' Sauvegarde modèle
	Elseif Trim(Lcase(CurrentDoc.f_ActionAFaire(0))) = "saveasmodel" Then
		Process.Action= PROCESSACTION_MODEL%
		
		' Si c'est un modèle déjà existant, on le sauvegarde
		If Cstr(CurrentDoc.f_EstModele(0)) = "1" Then
			Call SaveModel(CurrentDoc, CurrentUser)
			
		' Sinon, on vérifie que l'utilisateur peut sauver un nouveau modèle
		Elseif (CanUserSaveNewModel(CurrentDB, CurrentUser, CurrentDoc.f_TypeForm(0)))Then
			Call SaveModel(CurrentDoc, CurrentUser)
			
		' Sinon, on affiche le message "Vous ne pouvez pas enregistrer de nouveau modèle"			
		Else
			Process.Status = PROCESSSTATUS_FORBIDDEN%
		End If
		
		
	' Sauvegarde brouillon
	Elseif Trim(Lcase(CurrentDoc.f_ActionAFaire(0))) = "saveasdraft" Then
		Process.Action= PROCESSACTION_DRAFT%
		
		' Si c'est un brouillon déjà existant, on le sauvegarde
		If Cstr(CurrentDoc.f_EstBrouillon(0)) = "1" Then
			Call SaveDraft(CurrentDoc, CurrentUser)
			
		' Sinon, on vérifie que l'utilisateur peut sauver un nouveau brouillon
		Elseif (CanUserSaveNewDraft(CurrentDB, CurrentUser, CurrentDoc.f_TypeForm(0)))Then
			Call SaveDraft(CurrentDoc, CurrentUser)
			
		' Sinon, on affiche le message "Vous ne pouvez pas enregistrer de nouveau brouillon"			
		Else
			Process.Status = PROCESSSTATUS_FORBIDDEN%
		End If
		
	' A complémenter
	Elseif Trim(Lcase(CurrentDoc.f_ActionAFaire(0))) = "complementer" Then		
		Process.Action = PROCESSACTION_SUBMIT%
		Call GAMBPS_ComplementerDocument(CurrentDB, ParamsDocument, ProfileDocument, CurrentDoc, CurrentUser)		
		
	End If
	
	
	' On marque le document comme formulaire valide/non temporaire.
	CurrentDoc.Forms_TempDocument = "0"
	
	' On enregistre le document
	' /!\ l'utilisateur éxécutant l'agent est toujours auteur sur le document de contexte.
	' Mais quand on passe un document en paramètre (dans les sous-procédures de cet agent), et donc par valeur, 
	' les seuls auteurs sont définis dans les champs auteurs...
	Call CurrentDoc.Save(True, False)
	
	
	' On affiche le résultat
	Process.DocumentID = CurrentDoc.UniversalID
	'Call Process.DisplayStatus()
	
	Exit Sub
	
		' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	' 														GESTION D'ERREURS
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *	
ErrorHandler:
	
	Dim LogEntry As New LogEntry
	
	' Erreur propagée
	If (Err = ERROR_PROPAGATE%) Then
		
		' On charge les valeurs depuis le message d'erreur
		LogEntry.FromString(Error$)
		
		
	' Autres erreurs
	Else
		
		' On log l'erreur
		LogEntry.LogType			= LOGLEVEL_EMERG%
		LogEntry.SubName		= "InitializeDocument"
		LogEntry.Erl 			= Erl
		LogEntry.Err			= Err
		LogEntry.Msg			= Error$
		
	End If
	
	LogEntry.Component = "GAMBPS"
	
	' Si l'erreur n'a pas déjà été loggée (répétition de l'erreur)
	If Not Process.IsErrLogged(LogEntry) Then
		
		Call LogEntry.Save(ProfileDocument.f_Serveur_Log(0), ProfileDocument.f_Path_Log(0))
		
		' On ajoute l'erreur à la liste
		Call Process.AddLogEntry(LogEntry)
		
	End If
	
	' Warning, on continue
	If Not LogEntry.IsFatal Then
		Resume Next
		
	' Sinon, on affiche le résultat
	Else
'		Call Process.DisplayStatus()
		
	End If
	
	Exit Sub
	
End Sub<break/></text></item>
