<item name='$AgentHScript' summary='false' sign='true'><text>'++LotusScript Development Environment:2:5:(Options):0:74
Option Public
Option Declare

Use "Forms_Libs"
Use "Forms_Class_Process"

'++LotusScript Development Environment:2:5:(Forward):0:1
Declare Sub Initialize	
Declare Sub GAMBPS_StepUpDocument(pDB As NotesDatabase, pFormParamsDocument As NotesDocument, pProfileDocument As NotesDocument, pDocument As NotesDocument, pUserName As NotesName)
Declare Sub GAMBPS_DenyDocument(pDB As NotesDatabase, pFormParamsDocument As NotesDocument, pProfileDocument As NotesDocument, pDocument As NotesDocument, pUserName As NotesName)
Declare Sub GAMBPS_CancelDocument(pDB As NotesDatabase, pFormParamsDocument As NotesDocument, pProfileDocument As NotesDocument, pDocument As NotesDocument, pUserName As NotesName)
Declare Sub GAMBPS_NotifDocument(pDB As NotesDatabase, pFormParamsDocument As NotesDocument, pProfileDocument As NotesDocument, pDocument As NotesDocument, pUserName As NotesName)
Declare Sub GAMBPS_ComplementerDocument(pDB As NotesDatabase, pFormParamsDocument As NotesDocument, pProfileDocument As NotesDocument, pDocument As NotesDocument, pUserName As NotesName)
Declare Function CalculeNumeroOrdre(nOrdre As Long) As String
Declare Sub InitializeImport( Database As notesDatabase, ProfileDocument As NotesDocument, ContextDocument As notesdocument, Process As Process)
Declare Sub InitializeDocument( CurrentDB As notesDatabase, ProfileDocument As NotesDocument, CurrentDoc As NotesDocument, ContextDoc As NotesDocument, Process As Process)

'++LotusScript Development Environment:2:5:(Declarations):0:10
'This case determines whether it is a manual approval or from the file
Dim ApproveReject_File As Boolean

'++LotusScript Development Environment:2:2:Initialize:0:10
' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
' Procédure principale
' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
Sub Initialize	
	' Gestion d'erreurs	
	On Error Goto ErrorHandler
	
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	' 																INITIALISATION
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	' Init environnement
	Dim CurrentSession As New NotesSession
	Dim CurrentDoc As NotesDocument
	Dim CurrentDB As NotesDatabase
	Dim Process As New Process
	Dim ProfileDocument As NotesDocument
	
	Set CurrentDoc = CurrentSession.DocumentContext
	Set CurrentDB = CurrentSession.CurrentDatabase
	
	' Document de profil
	Set ProfileDocument = CurrentDB.GetProfileDocument("m_ProfileParam")
	If ProfileDocument Is Nothing Then Error lsERR_NOTES_NOSUCH_PROFILE, "Impossible d'ouvrir le document de profil !"
	
	' Conflit d'enregistrement
	If CurrentDoc.IsResponse Then
		Error ERROR_SAVECONFLICT%,"Conflit d'enregistrement"
		
	End If
	
	'Initialize case
	ApproveReject_File = False
	If CurrentDoc.GetItemValue("Form")(0) = "m_UIImportDialog" Then
		'Approval/Reject from file
		ApproveReject_File = True
		' We have a set of documents from an imported CSV file to process
		Call InitializeImport( CurrentDB, ProfileDocument, CurrentDoc, Process )
	Else
		' We have a single GAMBPS document to process
		Call InitializeDocument( CurrentDB, ProfileDocument, CurrentDoc, CurrentDoc, Process )
		Call Process.DisplayStatus()
	End If
	
	
	' C'est fini, on sort
	Exit Sub
	
	
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	' 														GESTION D'ERREURS
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *	
ErrorHandler:
	
	Dim LogEntry As New LogEntry
		
	' Erreur propagée
	If (Err = ERROR_PROPAGATE%) Then
		
		' On charge les valeurs depuis le message d'erreur
		LogEntry.FromString(Error$)
		
		
	' Autres erreurs
	Else
		
		' On log l'erreur
		LogEntry.LogType			= LOGLEVEL_EMERG%
		LogEntry.SubName		= "Initialize"
		LogEntry.Erl 			= Erl
		LogEntry.Err			= Err
		LogEntry.Msg			= Error$
		
	End If
	
	LogEntry.Component = "GAMBPS"	
	
	' Si l'erreur n'a pas déjà été loggée (répétition de l'erreur)
	If Not Process.IsErrLogged(LogEntry) Then
		
		Call LogEntry.Save(ProfileDocument.f_Serveur_Log(0), ProfileDocument.f_Path_Log(0))
		
		' On ajoute l'erreur à la liste
		Call Process.AddLogEntry(LogEntry)		
	End If
	
	
	' Warning, on continue
	If Not LogEntry.IsFatal Then
		Resume Next
		
	' Sinon, on affiche le résultat
	Else
		Call Process.DisplayStatus()		
	End If
	
	
	Exit Sub
	
End Sub

'++LotusScript Development Environment:2:2:GAMBPS_StepUpDocument:0:8
' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
' Cette procédure se charge de placer le document dans l'état suivant
' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
Sub GAMBPS_StepUpDocument(pDB As NotesDatabase, pFormParamsDocument As NotesDocument, pProfileDocument As NotesDocument, pDocument As NotesDocument, pUserName As NotesName)
	
	' Gestion d'erreurs	
	On Error Goto ErrorHandler
	
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	' 																INITIALISATION
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	
	' Variables locales
	Dim Formule As String
	Dim Res As Variant
	Dim strKey As String
	Dim dtTemp As NotesDateTime
	Dim strTemp As String	
	Dim listTemp List As String
	Dim boolRefuse As Boolean
	Dim boolTemp As Boolean
	Dim strCodePays As String
	Dim varPays As Variant
	
	'Add by: Juan Bautista Cabral. JIRA 720
	'Date: 19/04/2011
	Dim TypeMarque$
	
	'Attribuée: (FMR-602) GAMBPS : Validation d'étape par "import" 07/10/2010 by Juan Bautista Cabral.
	Dim UserApprovalFile As NotesName
	
	Dim ParentDocUID As Variant
	Dim ParentDoc As NotesDocument
	
	Dim CurrentStep As Integer
	
	Dim SendToActors List As NotesName
	Dim CopyToActors List As NotesName
	
	Dim itemAuthors As NotesItem
	Dim itemGAMBPSAuthorsByPays As NotesItem
	Dim TempItem As NotesItem, LectPotItem As NotesItem, AutPotItem As NotesItem
	Dim TempName As NotesName
	Dim AutreName As NotesName
	Dim iAutre As NotesItem
	Dim TempString As String
	Dim sNumOrdre As String
	Dim rol_Marque As String
	Dim rol_Admin As String
	Dim TempDocument As NotesDocument
	Dim Found As Boolean
	
	' Base de paramétrage
	Dim ParamDB As New NotesDatabase(pProfileDocument.f_Serveur_Parametre(0), pProfileDocument.f_Path_Parametre(0))
	If Not ParamDB.IsOpen Then Error lsERR_NOTES_DBOPEN_FAILED, "Impossible d'ouvrir la base """ +_
	pProfileDocument.f_Path_Parametre(0) +_
	""" sur le serveur """ + pProfileDocument.f_Serveur_Parametre(0) + """ !"
	
	' Vues de documents de paramétrage
	Dim ValidMarcheView As NotesView, ValidDPMXView As NotesView
	Dim ValidPDSTView As NotesView, ValidCRView As NotesView
	Dim ValidVehiculesZoneView As NotesView, vNumeros  As NotesView
	Dim vVehicules As NotesView
	
	'Add elements: Jira 720 by: Juan Bautista Cabral.
	Dim ValidGammeView As NotesView
	Dim validFRISCOView As NotesView
	
	
	'Documents de paramétrage
	Dim ValidGammeDocument As NotesDocument 'Add elements: Jira 720 by: Juan Bautista Cabral.
	Dim ValidMarcheDocument As NotesDocument, ValidDPMXDocument As NotesDocument
	Dim ValidPDSTDocument As NotesDocument, ValidCRDocument As NotesDocument
	Dim ValidVehiculeZoneDocument As NotesDocument, dNumero  As NotesDocument
	Dim DVehicule As Notesdocument
	
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	' 																TRAITEMENT
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	
	' Etat de la demande	
	CurrentStep = Cint(pDocument.f_Etat(0))
	
	
	' Initialisation
	If CurrentStep = 0 Then
		pDocument.f_CompterUtilisation = 1
		pDocument.IDDoc = pDocument.Universalid
	End If
	
	' Historique-
	'This case determines whether it is a manual approval or from the file
	'(Exception: Validation d'étape par "import" 07/10/2010 by Juan Bautista Cabral)
	'NL 29/02/2012: Fiche FMR-912 : This exception is no longer wanted
	%REM
	If ApproveReject_File Then
		Set vVehicules = ParamDB.GetView("(v_GAMBPS_Lookup_Vehicules)")		
		Set dVehicule = vVehicules.GetDocumentByKey(pDocument.GAMBPS_Vehicule(0), True)
		If Not dVehicule Is Nothing Then
			'RB 30/01/2012: Fiche FMR-912 : Indiquer l'utilisateur qui fait l'import
			set UserApprovalFile = New NotesName(pDocument.GAMBPS_Import_User(0))
			If Not UserApprovalFile Is Nothing Then
				Call AddHistLine(pDocument, "l_GAMBPS_Histo_E" + Cstr(CurrentStep), UserApprovalFile.Abbreviated)
			End If
		Else
			Error lsERR_NOTES_DOC_NOTINVIEW, "Impossible d'ouvrir le document de paramétrage ""Vehicule"" !"
		End If
	Else	
		Call AddHistLine(pDocument, "l_GAMBPS_Histo_E" + Cstr(CurrentStep), pUserName.Abbreviated)
	End If
	%END REM
	
	'NL 29/02/2012: Fiche FMR-912 : Back to the original (one line) version of Historique
	Call AddHistLine(pDocument, "l_GAMBPS_Histo_E" + CStr(CurrentStep), pUserName.Abbreviated)
	
	
	' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	
	' E0: Première soumission
	If (CurrentStep = 0) Then
		' Mettre à jour le champs du periode pour la recherche
		pDocument.GAMBPS_debut = pDocument.GAMBPS_moisDebut(0)+"/"+pDocument.GAMBPS_anneDebut(0)		
		pDocument.GAMBPS_fin = pDocument.GAMBPS_moisFin(0)+"/"+pDocument.GAMBPS_anneFin(0)
		
		TypeMarque$ = pDocument.GAMBPS_Marque(0)
		
		'Calcul du numero de demande
		If pDocument.A_Complementer(0) &lt;&gt; "O" Then
			'Set vNumeros = ParamDB.GetView("(v_GAMBPS_Lookup_Numeros)")
			'Set dNumero = vNumeros.GetDocumentByKey(TypeMarque$	+ pDocument.GAMBPS_Pays(0), True)
			'If dNumero Is Nothing Then Error lsERR_NOTES_DOC_NOTINVIEW, "Impossible d'ouvrir le document de paramétrage ""Numéros de demandes"" !"
			
			If pFormParamsDocument.GAMBPS_Numero_Ordre(0) &lt;&gt; "" Then
				sNumOrdre = CalculeNumeroOrdre(Clng(pFormParamsDocument.GAMBPS_Numero_Ordre(0))+1)
				varPays = Evaluate("@Elements(GAMBPS_Pays)", pDocument)
				If varPays(0) &gt; 1 Then
					strCodePays = "XXX"
				Else
					strCodePays = pDocument.GAMBPS_Pays(0)
				End If
				pDocument.GAMBPS_NumeroDemande = TypeMarque$ + strCodePays + sNumOrdre
			End If
			
			pFormParamsDocument.GAMBPS_Numero_Ordre = sNumOrdre
			Call pFormParamsDocument.save(True, False)
		End If
		
		Call GAMBPS_NotifDocument(pDB, pFormParamsDocument, pProfileDocument, pDocument, pUsername)
		
		' -------------------------------
		' Marque: AP=Peugeot / AC=Citroen
		' -------------------------------
		If UCase(TypeMarque$) = "AP" Then
			' Ouverture de la vue contenant le document "Chef de Marche"
			Set ValidMarcheView = ParamDB.GetView("v_GAMBPS_ValidMarche_Lookup_ByKey")
			If ValidMarcheView Is Nothing Then Error lsERR_NOTES_VIEWOPEN_FAILED, "Impossible d'ouvrir la vue ""v_GAMBPS_ValidMarche_Lookup_ByKey"" sur la base de paramétrage !"
			
			' Auteurs
			Set itemAuthors = pDocument.ReplaceItemValue("f_AuteursPotentiels", "")
			
			' Auteurs par pays
			Set itemGAMBPSAuthorsByPays = pDocument.ReplaceItemValue("GAMBPS_AuthorsByPays_E1", "")
			
			' Resets parallel validation fields
			Call pDocument.ReplaceItemValue("GAMBPS_ValidatedPays_E1", "")
			Call pDocument.ReplaceItemValue("GAMBPS_SubmittedPays_E1", "")
			
			' Boucle sur les pays/bâtiments sélectionnés par le demandeur
			ForAll strTemp1 In pDocument.GAMBPS_Pays
				strKey = "AP-"+strTemp1

				' Document "Chef de Marché" (en théorie, un seul document de ce type existe)
				Set ValidMarcheDocument = ValidMarcheView.GetDocumentbykey(strKey, True)
				If ValidMarcheDocument Is Nothing Then Error lsERR_NOTES_DOC_NOTINVIEW, "Impossible d'ouvrir le document de paramétrage ""Chef de Marche"" !"
				
				' Responsable de pays en auteur
				' Correspondant Chef de marché
				Set TempItem = ValidMarcheDocument.GetFirstItem("GAMBPS_ValidMarche_Name")
				If TempItem Is Nothing Then Error lsERR_NOTES_ITEM_MISSING, "Impossible de trouver le champ de paramétrage ""GAMBPS_ValidMarche_Name"" !"
				Call itemAuthors.AppendToTextList(TempItem.Values)
				
				' Auteurs par pays
				Call itemGAMBPSAuthorsByPays.AppendToTextList(strTemp1 + "§" + Implode(TempItem.Values, "§"))
				
				' Envoi du mail (BAL nominative ou fonctionnelle)
				If Trim(UCase(ValidMarcheDocument.GAMBPS_SendMail(0))) = "BALNOM" Then
					Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, ValidMarcheDocument.GAMBPS_ValidMarche_Name, Null, "VALID")
					Call pDocument.ReplaceItemValue("Forms_LastValidSendTo", ValidMarcheDocument.GAMBPS_ValidMarche_Name)
				ElseIf Trim(UCase(ValidMarcheDocument.GAMBPS_SendMail(0))) = "BALFON" Then
					Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, ValidMarcheDocument.GAMBPS_ValidMarche_Balfon, Null, "VALID")
					Call pDocument.ReplaceItemValue("Forms_LastValidSendTo", ValidMarcheDocument.GAMBPS_ValidMarche_Balfon)
				End If
				
			End ForAll
			
			' Mise en forme des auteurs et derniers destinataires des mails de validation
			pDocument.f_AuteursPotentiels = Evaluate(|@Sort(@Unique(@Trim(f_AuteursPotentiels)))|, pDocument)
			pDocument.Forms_LastValidSendTo = Evaluate(|@Sort(@Unique(@Trim(Forms_LastValidSendTo)))|, pDocument)
			
			'Modified 24/11/2010 By: Juan Bautista Cabral JIRA 662
			'Lecteurs: ajout des valideurs et des personnes en copie
			pDocument.f_LecteursPotentiels = Evaluate(|@Sort(@Unique(@Trim(f_LecteursPotentiels : GAMBPS_PeopleToInform : f_AuteursPotentiels)))|, pDocument)
		
			'Changement d'état, etat 1
			Call ChangeState(pDocument, pFormParamsDocument, 1, "Intermediaire")
			
			'Dispaching des acteurs (groupes ou individus)
			Call DispatchActors(pDocument)
			
		ElseIf UCase(TypeMarque$) = "AC" Then
			'Ouverture de la vue contenant le document "Chef de Gamme"
			Set ValidGammeView = ParamDB.GetView("v_GAMBPS_ValidGamme")
			If ValidGammeView Is Nothing Then Error lsERR_NOTES_VIEWOPEN_FAILED, "Impossible d'ouvrir la vue ""v_GAMBPS_ValidGamme"" sur la base de paramétrage !"
				
			'Document "Chef de Gamme" (en théorie, un seul document de ce type existe)
			Set ValidGammeDocument = ValidGammeView.GetDocumentbykey(pDocument.GAMBPS_Vehicule(0), True)
			If ValidGammeDocument Is Nothing Then Error lsERR_NOTES_DOC_NOTINVIEW, "Impossible d'ouvrir le document de paramétrage ""Chef de Gamme"" !"

			'Correspondant Chef de Gamme
			Set TempItem = ValidGammeDocument.GetFirstItem("GAMBPS_ValidGamme_Name")
			If TempItem Is Nothing Then Error lsERR_NOTES_ITEM_MISSING,_
			"Impossible de trouver le champ de paramétrage ""GAMBPS_ValidGamme_Name"" !"
			
			Call pDocument.ReplaceItemValue("f_AuteursPotentiels", TempItem.Values)
				
			'Lecteurs: ajout des valideurs
			rol_Marque = "[GAMBPS_Reader"+TypeMarque$+"]"
			rol_Admin = "[GAMBPS_RESP]"
			
			If (pDocument.GAMBPS_Autre_per_hab(0) &lt;&gt; "" ) Then
				Set AutreName = New NotesName(pDocument.GAMBPS_Autre_per_hab(0))
				pDocument.f_LecteursPotentiels = Evaluate(|@Sort(@Unique(@Trim("|+_
				rol_Admin+|":"|+rol_Marque+|":f_LecteursPotentiels:"|+AutreName.Canonical+|": f_AuteursPotentiels)))|, pDocument)
			Else
				pDocument.f_LecteursPotentiels = Evaluate(|@Sort(@Unique(@Trim("|+_
				rol_Admin+|":"|+rol_Marque+|":f_LecteursPotentiels : f_AuteursPotentiels)))|, pDocument)
			End If
			
			'Modified 24/11/2010 By: Juan Bautista Cabral JIRA 662
			'Lecteurs: ajout des valideurs et des personnes en copie 
			pDocument.f_LecteursPotentiels = Evaluate(|@Sort(@Unique(@Trim(f_LecteursPotentiels : GAMBPS_PeopleToInform)))|, pDocument)
			
			'Changement d'état, etat 2
			Call ChangeState(pDocument, pFormParamsDocument, 2, "Intermediaire")
			
			'Dispaching des acteurs (groupes ou individus)
			Call DispatchActors(pDocument)
			
			'Envoi du mail (BAL nominative ou fonctionnelle)
			If Trim(UCase(ValidGammeDocument.GAMBPS_SendMail(0))) = "BALNOM" Then
				Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, ValidGammeDocument.GAMBPS_ValidGamme_Name, Null, "VALID")
				Call pDocument.ReplaceItemValue("Forms_LastValidSendTo", ValidGammeDocument.GAMBPS_ValidGamme_Name)
			ElseIf Trim(UCase(ValidGammeDocument.GAMBPS_SendMail(0))) = "BALFON" Then
				Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, ValidGammeDocument.GAMBPS_ValidGamme_Balfon(0), Null, "VALID")
				Call pDocument.ReplaceItemValue("Forms_LastValidSendTo", ValidGammeDocument.GAMBPS_ValidGamme_Balfon(0))
			End If
		End If
		
		' Modified 24/11/2010 By: Juan Bautista Cabral JIRA 662
		' Envoi d'un mail de notification aux personnes en copie
		If Trim(pDocument.GAMBPS_PeopleToInform(0)) &lt;&gt; "" Then
