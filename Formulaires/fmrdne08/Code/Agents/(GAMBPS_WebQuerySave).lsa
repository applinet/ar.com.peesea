	' 																TRAITEMENT
	' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
	
	' Historique-
	'This case determines whether it is a manual approval or from the file
	'(Exception: Validation d'étape par "import" 07/10/2010 by Juan Bautista Cabral)
	'NL 29/02/2012: Fiche FMR-912 : This exception is no longer wanted
	%REM
	If ApproveReject_File Then
		Set vVehicules = ParamDB.GetView("(v_GAMBPS_Lookup_Vehicules)")		
		Set dVehicule = vVehicules.GetDocumentByKey(pDocument.GAMBPS_Vehicule(0), True)
		If Not dVehicule Is Nothing Then
			'RB 30/01/2012: Fiche FMR-912 : Indiquer l'utilisateur qui fait l'import
			Set UserRejectFile = New NotesName(pDocument.GAMBPS_Import_User(0))
			If Not UserRejectFile Is Nothing Then
				Call AddHistLine(pDocument, "l_HRefuse", UserRejectFile.Abbreviated)
			End If
		Else
			Error lsERR_NOTES_DOC_NOTINVIEW, "Impossible d'ouvrir le document de paramétrage ""Vehicule"" !"
		End If
	Else	
		Call AddHistLine(pDocument, "l_HRefuse", pUserName.Abbreviated)
	End If
	%END REM
	
	'NL 29/02/2012: Fiche FMR-912 : Back to the original (one line) version of Historique
	Call AddHistLine(pDocument, "l_HRefuse", pUserName.Abbreviated)
	
	Set iLect = pDocument.GetFirstItem("f_LecteursPotentiels")
	
	' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	
	' E1: Valideur autorisé =&gt; Chef de Marché OR Chef de Gamme
	If (CurrentStep &gt;= 1) Then
		
		' Lecteurs: ajout des valideurs
		Call iLect.AppendToTextList(Demandeur.Canonical)	
		' Envoi du mail (Demandeur)
		Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, 	Demandeur.Canonical, Null, "KO")
		
		If (pDocument.GAMBPS_Autre_per_hab(0) &lt;&gt; "" ) Then
			Set AutrePersonneHab = New NotesName(pDocument.GAMBPS_Autre_per_hab(0))
			' Lecteurs: ajout des valideurs
			Call iLect.AppendToTextList(AutrePersonneHab.Canonical)	
			' Envoi du mail (Autre Personne habilitée)
			Call SendPresetMail(pDB, pFormParamsDocument, pProfileDocument, pDocument, AutrePersonneHab.Canonical, Null, "KO")
		End If
		
		pDocument.f_LecteursPotentiels = Evaluate(|@Sort(@Unique(@Trim(f_LecteursPotentiels)))|, pDocument)
	End If
	
	TypeMarque$ = pDocument.GAMBPS_Marque(0)
		
	' Marque: AP=Peugeot / AC=Citroen
	If UCase(TypeMarque$) = "AP" Then
		
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	
		' E3: Valideur autorisé =&gt; Chef produit DPMX
		If (CurrentStep &gt;= 3) Then
			
			' Ouverture de la vue contenant le document "Chef de Marche"
			Set ValidMarcheView = ParamDB.GetView("v_GAMBPS_ValidMarche_Lookup_ByKey")
